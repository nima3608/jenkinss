pipeline {
    
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('By') {
            steps {
                echo 'By World'
            }
        }
        stage('OK') {
            steps {
                sleep 5
                echo 'I\'m ok'
            }
        }
    }
}